{
  "presets": [
    ["env", {
      "targets": {
        "browsers": "last 2 versions"
      },
      "useBuiltIns": true,
      "debug": false
    }]
  ],
  "plugins": ["transform-es3-property-literals", "babel-plugin-transform-es3-member-expression-literals"],
  "ignore": ["node_modules"],
  "retainLines": true
}
